{
    "name": "AutoRename",
    "short_name": "AutoRename",
    "version": "1.0.1",
    "description": "AutoRename can automatically rename downloaded images from the internet.",
    "default_locale": "en",
    "manifest_version": 2,
    "homepage_url": "https://github.com/ddasutein/AutoRename",
    "icons": {
	    "48": "/assets/AutoRename-Logo-48px.png",
        "128": "/assets/AutoRename-Logo-128px.png"
    },
    "browser_action":{
        "default_title": "AutoRename"
    },
    "background": {
        "scripts": ["js/saveImageEvent.js"],
        "persistent": false
    },

    "options_page": "options.html",

    "permissions":[
        "storage",
        "contextMenus",
        "downloads"
    ]
}